# Stellarealize Environment Variables

# 1inch API Configuration
NEXT_PUBLIC_ONE_INCH_API_KEY=your_1inch_api_key_here

# Stellar Network Configuration
STELLAR_NETWORK=TESTNET
STELLAR_RPC_URL=https://horizon-testnet.stellar.org
SOROBAN_RPC_URL=https://soroban-testnet.stellar.org

# Production URLs (for mainnet)
# STELLAR_NETWORK=MAINNET
# STELLAR_RPC_URL=https://horizon.stellar.org
# SOROBAN_RPC_URL=https://soroban-rpc.mainnet.stellar.gateway.fm

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Stellarealize

# Analytics (Optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_MIXPANEL_TOKEN=

# Development (Optional)
DEBUG=false
LOG_LEVEL=info